class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        i = 0
        j = i + 1
        sum = 0
        o = []
        while i < len(nums):
            while j < len(nums):

                sum = nums[i] + nums[j]
                if(sum == target):
                    o.append(i)
                    o.append(j)
                    return o
                j = j + 1
            i = i + 1
            j = i + 1
        return o
                
                
